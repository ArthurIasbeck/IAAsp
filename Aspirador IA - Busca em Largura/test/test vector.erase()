std::vector<int> test;
test.push_back(1);
test.push_back(2);
test.push_back(3);
test.push_back(4);

std::cout << "Antes de apagar" << std::endl;
for(int i = 0; i < test.size(); i++){
std::cout << "Elemento " << i << ": " << test[i] << std::endl;
}

test.erase(test.begin() + 1);

std::cout << "\nDepois de apagar" << std::endl;
for(int i = 0; i < test.size(); i++){
std::cout << "Elemento " << i << ": " << test[i] << std::endl;
}
